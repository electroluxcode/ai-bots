  "dependencies": {
    "@ai-bots/types": "workspace:*",
    "@ai-bots/node-base": "workspace:*",
    "@ai-bots/node-model": "workspace:*",
    "@ai-bots/configs": "workspace:*",
    "@ai-bots/utils": "workspace:*",
    "@ai-bots/node-workflow": "workspace:*",
    "@ai-bots/node-tasks": "workspace:*",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@ai-bots/configs": "workspace:*",
    "@types/node": "^20.17.30",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}